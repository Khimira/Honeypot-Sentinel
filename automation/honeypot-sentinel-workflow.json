      "parameters": {
        "jsCode": "// Safely extract command from \"Format Fields\" node to avoid case errors\nconst command = $('Format Fields').item.json.Command || \"\";\n\n// Enhanced regex to capture URLs\nconst urlRegex = /(https?:\\/\\/[^\\s\"';<>]+)/gi;\nconst foundUrls = command.match(urlRegex);\n\nlet cleanUrl = null;\nif (foundUrls && foundUrls.length > 0) {\n  // Remove periods or commas that the attacker may have typed at the end of the URL\n  cleanUrl = foundUrls[0].replace(/[.,\"';<>]$/g, '');\n}\n\nreturn {\n  json: {\n    ...$input.item.json, // Keep spreadsheet data\n    url_detected: cleanUrl,\n    contains_url: !!cleanUrl\n  }\n};"
      },
